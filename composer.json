{
    "name": "albert-anderberg/oauth2-clickup",
    "description": "ClickUp OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Albert Anderberg",
            "email": "albert@aanet.se",
            "homepage": "https://github.com/albert-anderberg"
